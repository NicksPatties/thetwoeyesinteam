<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
android:id="@+id/main_wrapper"
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:facebook="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".ResultPage">

    <RelativeLayout
    android:id="@+id/result_page_wrapper"
	android:layout_width="720px"
	android:layout_height="1280px"
	android:layout_gravity="center">
	
        <ImageView
		android:id="@+id/bg"
		android:scaleType="centerCrop"
		android:adjustViewBounds="true"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:src="@drawable/bg_challenge_results_720_1280" >
		</ImageView>
		
        <Button
        android:id="@+id/btn_home_result"
        android:layout_width="70px"
        android:layout_height="70px"
        android:background="@drawable/home_button_bg"
        android:clickable="true"
        android:textColor="@android:color/white"
        android:onClick="goHome"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="60px"
        android:layout_marginLeft="68px" />  	    
	    
   	    <Button
        android:id="@+id/btn_FB_result"
        android:layout_weight="1"
        android:layout_width="70px"
        android:layout_height="70px"
        android:background="@drawable/help_button_bg"
        android:clickable="true"
        android:textColor="@android:color/white"
        android:onClick="goFacebook"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="60px"
        android:layout_marginRight="68px" />	  
   	    
   	    <TableLayout 
		android:id="@+id/result_table"
		android:layout_width="640px"
		android:layout_height="770px"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="140px">  
		</TableLayout>
		
		<Button
	    android:id="@+id/btn_next_round"
	    android:layout_width="640px"
        android:layout_height="100px"        
        android:background="@drawable/button_bg"
	    android:textColor="@android:color/white"
    	android:textSize="20px"
    	android:textStyle="bold"
        android:layout_gravity="center_horizontal"
        android:text="NEXT ROUND"
	    android:onClick="onNextRound"
	    android:layout_centerHorizontal="true"
	    android:layout_alignParentBottom="true"
	    android:layout_marginBottom="220px" />
	    
	</RelativeLayout>
	
</FrameLayout>


