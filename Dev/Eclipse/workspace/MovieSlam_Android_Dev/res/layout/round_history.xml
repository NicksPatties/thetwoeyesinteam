<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
android:id="@+id/main_wrapper"
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:facebook="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".RoundHistory">

    <RelativeLayout
    android:id="@+id/round_history_wrapper"
	android:layout_width="720px"
	android:layout_height="1280px"
	android:layout_gravity="center">
	
        <ImageView
		android:id="@+id/bg"
		android:scaleType="centerCrop"
		android:adjustViewBounds="true"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:src="@drawable/bg_challenge_results_popup_720_1280" >
		</ImageView>
		
        <ScrollView
		android:layout_width="642px"
		android:layout_height="1150px"
		android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" >
				                  
			<TableLayout
			android:id="@+id/round_history_table"
	        android:layout_width="642px"
	        android:layout_height="1150px"
	        android:layout_alignParentBottom="true"
	        android:layout_centerHorizontal="true">  
			</TableLayout>
		        	
		</ScrollView>
		
	</RelativeLayout>
	
</FrameLayout>
