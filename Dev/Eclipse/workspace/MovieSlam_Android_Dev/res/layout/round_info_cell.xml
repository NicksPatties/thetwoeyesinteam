<?xml version="1.0" encoding="utf-8"?>  

<TableRow
xmlns:android="http://schemas.android.com/apk/res/android"  
android:id="@+id/tableRow1"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:background="@drawable/panel_challenge_results"
android:layout_marginLeft="11dp"
android:layout_marginRight="11dp"
android:layout_marginTop="12dp" >				 
      
	<LinearLayout
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:layout_weight="1">
         
		<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:layout_weight="1">
		</LinearLayout>
         
		<RelativeLayout
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:layout_weight="6">
		    
			
			<ImageView
	        android:id="@+id/movie_tn"
	        android:paddingLeft="20dp"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentTop="true"
	        android:layout_width="100dp"
	        android:layout_height="100dp"
	        android:adjustViewBounds="true"
	        android:scaleType="centerCrop" /> 
	        
			<TextView
		    android:id="@+id/movie_txt"
		    android:layout_width="wrap_content"
		    android:layout_alignTop="@+id/movie_tn"
		    android:layout_toRightOf="@id/movie_tn"
		    android:layout_toLeftOf="@+id/round_player_tn"
		    android:layout_marginLeft="10dp"
		    android:layout_height="wrap_content"
		    android:text=""
		    android:textColor="#18305c"
		    android:textSize="12sp"
		    android:textStyle="bold" />
			
			<TextView
		    android:id="@+id/buy_txt"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"		    
		    android:layout_below="@id/movie_txt"
		    android:layout_alignLeft="@id/movie_txt"	
		    android:layout_alignRight="@id/movie_txt"	    
		    android:text="BUY NOW"
		    android:textColor="#FFFFFF"
		    android:textSize="12sp"
		    android:textStyle="bold" />
			
			<ImageView
	        android:id="@+id/round_user_tn"
	        android:layout_width="70dp"
	        android:layout_height="70dp"
	        android:layout_toLeftOf="@id/round_player_tn"
	        android:layout_alignParentTop="true"
	        android:adjustViewBounds="true"
	        android:layout_marginRight="20dp"
	        android:scaleType="centerCrop" />
			
			<TextView
			android:gravity="center_horizontal"
		    android:id="@+id/round_user_txt"
		    android:layout_width="wrap_content"
		    android:layout_below="@id/round_user_tn"
		    android:layout_alignLeft="@id/round_user_tn"
		    android:layout_alignRight="@id/round_user_tn"
		    android:layout_height="wrap_content"
		    android:text=""
		    android:textColor="#FFFFFF"
		    android:textSize="12sp"
		    android:textStyle="bold" />
	        
			<ImageView
		    android:id="@+id/round_player_tn"
		    android:layout_width="70dp"
		    android:layout_height="70dp"
		    android:layout_alignParentRight="true"
		    android:layout_alignParentTop="true"
		    android:layout_marginRight="20dp"
		    android:adjustViewBounds="true"
		    android:scaleType="centerCrop" />
			
			<TextView
			android:gravity="center_horizontal"
		    android:id="@+id/round_player_txt"
		    android:layout_width="wrap_content"
		    android:layout_below="@id/round_player_tn"
		    android:layout_alignLeft="@+id/round_player_tn"
		    android:layout_alignRight="@+id/round_player_tn"
		    android:layout_height="wrap_content"
		    android:text=""
		    android:textColor="#FFFFFF"
		    android:textSize="12sp"
		    android:textStyle="bold" />
        			            
		</RelativeLayout>
   
		<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="0dp"
		android:layout_weight="1">
		</LinearLayout>
		
	</LinearLayout>
         
</TableRow>